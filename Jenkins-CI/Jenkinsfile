pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }
        stage('Install') {
            steps {
                 bat 'cd Jenkins-CI & npm install'
            }
        }
        stage('Test') {
            steps {
                bat 'cd Jenkins-CI &  npm test'
            }
        }
    }
}